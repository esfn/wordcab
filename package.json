{
  "name": "wordcab",
  "version": "1.0.0",
  "description": "Learn languages with frequency lists.",
  "main": "index.html",
  "scripts": {
    "build:css": "node-sass --output-style compressed -o public/css src/scss",
    "build:js": "uglifyjs -o --compress --mangle --output public/js/bundle.min.js -- src/js/models/wordcabModel.js src/js/controllers/wordcabController.js src/js/views/wordcabView.js",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "onchange 'src/scss' -- npm run build:css",
    "watch:js": "onchange 'src/js/' -- npm run build:js",
    "watch": "npm-run-all --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esfn/wordcab.git"
  },
  "keywords": [
    "learn",
    "vocabulary",
    "languages",
    "frequency",
    "lists"
  ],
  "author": "esfn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/esfn/wordcab/issues"
  },
  "homepage": "https://github.com/esfn/wordcab#readme",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "uglify-es": "^3.3.9"
  }
}
